{
  "name": "@steerprotocol/strategy-utils",
  "version": "2.1.9",
  "description": "Strategy utilities library for Steer Protocol strategies",
  "main": "assembly/index.ts",
  "types": "assembly/index.ts",
  "scripts": {
    "test": "yarn jest",
    "asbuild:debug": "asc assembly/index.ts --target debug",
    "asbuild:release": "asc assembly/index.ts --target release",
    "asbuild": "npm run asbuild:debug && npm run asbuild:release",
    "start": "npx serve .",
    "docs": "typedoc --tsconfig ./tsconfig.json",
    "strategy": "yarn asbuild && yarn test",
    "semantic-release": "semantic-release",
    "commit": "cz",
    "postinstall": "patch-package"
  },
  "author": "Derek Barrera <derekbarrera@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@babel/preset-typescript": "^7.14.5",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@steerprotocol/app-loader": "^0.2.2",
    "@steerprotocol/base-strategy": "0.1.0",
    "as-bignum": "^0.2.23",
    "commitizen": "^4.2.4",
    "conventional-changelog-conventionalcommits": "^4.6.0",
    "json-as": "^0.5.43",
    "patch-package": "^8.0.0",
    "typescript": "^4.3.5",
    "visitor-as": "^0.11.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.14.8",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.11.11",
    "assemblyscript": "^0.27.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^27.0.6",
    "cz-conventional-changelog": "3.3.0",
    "jest": "^27.0.6",
    "regenerator-runtime": "^0.13.9",
    "semantic-release": "^17.4.5",
    "typedoc": "^0.21.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
